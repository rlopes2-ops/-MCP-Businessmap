# Businessmap (Kanbanize) Configuration
BUSINESSMAP_URL=https://sua-instancia.kanbanize.com
BUSINESSMAP_APIKEY=SUA_API_KEY

# SSL Verification (default is true, set to "false" to disable)
BUSINESSMAP_SSL_VERIFY=true

# Boards Filter (comma-separated list of board IDs to limit access to)
#BUSINESSMAP_BOARDS_FILTER=123,456,789

# Logging Level (INFO, DEBUG, WARNING, ERROR)
#MCP_VERBOSE=true

# Read-only Mode (set to "true" to disable all write operations)
#READ_ONLY_MODE=false 